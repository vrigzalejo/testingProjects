{
  "name": "laravel/lumen",
  "description": "The Laravel Lumen Framework.",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/lumen-framework": "5.1.*",
    "vlucas/phpdotenv": "~1.0",
    "guzzlehttp/guzzle": "^6.0",
    "illuminate/redis": "^5.1",
    "fzaninotto/faker": "^1.5",
    "fluent/logger": "v0.3.7"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "GuzzleHttp\\": "/vendor/guzzlehttp",
      "ConsumerCloud\\Brigs\\Repositories\\": "ConsumerCloud/Brigs/Repositories/src/",
      "ConsumerCloud\\Lgs\\Repositories\\": "ConsumerCloud/Brigs/Repositories/src/"
    },
    "classmap": [
      "database/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "app/",
      "GuzzleHttp\\": "/vendor/guzzlehttp",
      "ConsumerCloud\\Brigs\\Repositories\\": "ConsumerCloud/Brigs/Repositories/tests/",
      "ConsumerCloud\\Lgs\\Repositories\\": "ConsumerCloud/Brigs/Repositories/tests/"
    },
    "classmap": [
      "tests/"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
